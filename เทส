-- ======================== SETTING ========================
local settingPage = venyx:addPage("Setting", 6031280882)
local autoSection = settingPage:addSection("Setting AutoFarm")
local musicSection = settingPage:addSection("Music")

autoSection:addToggle("Auto Enchancement Haki", true, function(value)
    _G.AutoHaki = value
end)

autoSection:addKeybind("Toggle Keybind", Enum.KeyCode.RightControl, function()
    venyx:toggle()
end)

-- ======================== ANTI-AFK ========================
local vu = game:GetService("VirtualUser")
game.Players.LocalPlayer.Idled:Connect(function()
    vu:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
end)

-- ======================== AUTO HAKI LOOP ========================
spawn(function()
    while wait(0.1) do
        if _G.AutoHaki then
            if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
            end
        end
    end
end)

-- ======================== SIMULATION RADIUS ========================
function bring2()
    local plr = game.Players.LocalPlayer
    pcall(function()
        sethiddenproperty(plr,"SimulationRadius",math.huge)
    end)
end
spawn(function()
    while wait(0.1) do
        if _G.AutoFarm then
            bring2()
        end
    end
end)

-- ======================== AUTO FARM LOOP ========================
spawn(function()
    while true do
        pcall(function()
            if _G.AutoFarm and StatrMagnet then
                CheckQuest()
                for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        if syn and isnetworkowner(v.HumanoidRootPart) then
                            if isInRange(v,400) then
                                prepareEnemy(v, HideHitBlox)
                                v.HumanoidRootPart.CFrame = ToMob
                            end
                        else
                            if isInRange(v,400) then
                                prepareEnemy(v, HideHitBlox)
                                v.HumanoidRootPart.CFrame = ToMob
                            end
                        end
                    end
                end
            end
        end)
        wait()
    end
end)

-- ======================== THEME ========================
local themePage = venyx:addPage("Theme", 5012543246)
local colorSection = themePage:addSection("Colors")
for themeName, color in pairs(themes) do
    colorSection:addColorPicker(themeName, color, function(color3)
        venyx:setTheme(themeName,color3)
    end)
end

venyx:SelectPage(venyx.pages[1], true)
